function JReturn($message = '成功', $statusCode = 1, $data = array()) {
	$rs = array (
			'message' => $message,
			'statusCode' => $statusCode 
	);
	$rs = json_encode ( array_merge ( $rs, $data ), JSON_UNESCAPED_UNICODE );
	exit ( $rs );
}

